--Properties:

ScreenGui.Parent = game.CoreGui

main.Name = "main"
main.Parent = ScreenGui
main.Active = true
main.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
main.BorderSizePixel = 0
main.Position = UDim2.new(0.35158056, 0, 0.0944785178, 0)
main.Size = UDim2.new(0, 535, 0, 283)
main.Visible = true
main.Draggable = true

